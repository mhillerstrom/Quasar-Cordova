{
  "name": "quasar-cordova",
  "version": "0.0.1",
  "description": "A Quasar Framework app",
  "productName": "QuasarCordovaApp",
  "cordovaId": "org.cordova.quasar.app",
  "capacitorId": "",
  "main": "server",
  "keywords": [
    "feathers",
    "quasar",
    "cordova",
    "mongodb",
    "mobile-first"
  ],
  "author": {
    "name": "Michael HillerstrÃ¶m",
    "email": "michael@hillerstrom.name"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "server",
    "test": "test/",
    "config": "config/"
  },
  "private": true,
  "scripts": {
    "build:server:once": "cross-env NODE_ENV=development webpack --config webpack.config.server.js",
    "build:server:prod:once": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "dev:server": "npm run build:server:once && npm-run-all --parallel nodemon:dev watch:server",
    "watch:server": "cross-env NODE_ENV=development webpack --config webpack.config.server.js --watch",
    "nodemon:dev": "cross-env NODE_ENV=development nodemon --inspect ./server",
    "nodemon:prod": "npm run build:server:prod:once && cross-env NODE_ENV=production nodemon dist/server.prod.js",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ./",
    "test": "npm-run-all eslint mocha",
    "eslint": "eslint server/. test/. --config .eslintrc.json",
    "oldstart": "pm2 start server/ --no-daemon",
    "start": "COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 NODE_ENV=development docker-compose up",
    "build": "gulp && NODE_ENV=production webpack --config webpack.config.server.js && NODE_ENV=production npm-run-all build-android build-browser build-ios",
    "watch": "gulp && webpack  --config webpack.config.server.js -w",
    "make-config": "node config/mkconfig.js",
    "serve": "nodemon --inspect=0.0.0.0:9222 --watch server --watch config server/",
    "org-dev": "npm-run-all --parallel watch serve",
    "new-dev": "npm-run-all make-config --parallel watch serve",
    "cordova-watch": "NODE_ENV=development webpack -mode \"development\" -w",
    "cordova-serve": "NODE_ENV=development nodemon --inspect=0.0.0.0:9222  --watch server --watch config server/",
    "cordova-prepare": "cordova prepare browser",
    "quasar-serve": "NODE_ENV=development DATABASE_URL=mongodb://192.168.1.70:27017/MyAppDevDB nodemon --inspect=0.0.0.0:9222  --watch server --watch config server/",
    "quasar-dev": "quasar dev --debug",
    "xdev": "gulp && npm-run-all make-config --parallel cordova-watch cordova-serve",
    "mydev": "npm-run-all --parallel quasar-serve",
    "dockerdev": "node config/mkconfig.js && /bin/bash -c \"quasar dev &\" && COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 NODE_ENV=development docker-compose up",
    "dockerbuild": "node config/mkconfig.js COMPOSE_DOCKER_CLI_BUILD=1 DOCKER_BUILDKIT=1 NODE_ENV=production docker-compose up",
    "mocha": "mocha test/ --recursive --exit"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.5.4",
    "@feathersjs/authentication-local": "^4.5.4",
    "@feathersjs/authentication-oauth": "^4.5.1",
    "@feathersjs/configuration": "^4.5.3",
    "@feathersjs/rest-client": "^4.5.4",
    "@feathersjs/socketio": "^4.5.4",
    "@feathersjs/socketio-client": "^4.5.4",
    "@quasar/babel-preset-app": "^2.0.0",
    "@quasar/extras": "^1.8.2",
    "acorn": "^7.2.0",
    "ajv": "^6.12.2",
    "axios": "^0.18.1",
    "babel-polyfill": "^6.26.0",
    "bufferutil": "^4.0.1",
    "core-js": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-authentication-management": "^2.0.1",
    "feathers-hooks-common": "^5.0.3",
    "feathers-localstorage": "^5.1.0",
    "feathers-mailer": "^3.0.1",
    "feathers-mongodb": "^6.1.0",
    "feathers-mongoose": "^8.3.0",
    "feathers-offline-realtime": "^0.1.2",
    "helmet": "^3.22.0",
    "mongodb": "^3.5.8",
    "mongodb-core": "^3.2.7",
    "mongoose": "^5.9.17",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport-auth0": "^1.3.2",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-google-oauth20": "^2.0.0",
    "pug": "^2.0.4",
    "quasar": "^1.12.3",
    "serve-favicon": "^2.5.0",
    "socket.io-client": "^2.3.0",
    "utf-8-validate": "^5.0.2",
    "vue-stash": "^2.0.1-beta",
    "winston": "^3.2.1",
    "yup": "^0.28.5"
